            <input type="text" name="name" id="searchinput" class="form-control" placeholder = "What Qualities would you like?" >           
        <button type="submit" class="btn btn-primary" id="submitButton">Post It!</button>
        </form>

  <!-- adds list from database to ejs -->
      <ul class="list-group" id="list-ul">
    <% for (var i=0; i<qualities.length; i++) { %>
      <li class="list-group-item">
        <h4 class="list-group-item-heading"><%= qualities[i].name %></h4>
          <button data-id="<%=qualities[i].id %>" type='button' class='close' aria-label='Close'><span aria-hidden='true'>&times;</span></button>
      </li>
    <% } %>
    </ul>


